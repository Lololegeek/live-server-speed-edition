{
  "name": "live-server-speed-edition",
  "displayName": "Live Server Speed Edition",
  "description": "Preview your local web project inside a VS Code WebView or Default Browser with live reload.",
  "version": "1.0.9",
  "publisher": "Lololegeek",
  "icon": "icon.png",
  "repository": "https://github.com/Lololegeek/live-server-speed-edition",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fast-http-server.toggleServer",
        "title": "Start Fast HTTP Server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.5.0",
    "@types/open": "^6.1.0",
    "@types/ws": "^8.5.4",
    "mocha": "^11.7.1",
    "typescript": "^5.2.0",
    "vscode": "^1.1.35"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "open": "^10.2.0",
    "ws": "^8.13.0"
  }
}
